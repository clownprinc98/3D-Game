            <string>/// Parses the room and creates a floor with paths.

// assume 32*32 floors.

floorLocations = ds_map_create();
var curPos; // curPos[0] is x; curPos[1] is y;
var location = "";
curPos[0] = 0;
curPos[1] = 0;

while (curPos[1] &lt; room_height){

    // if 0 then floor isnt exists at that location
    if(ds_map_find_value(floorLocations, string(curPos[0]) + string(curPos[1])) == 0){
        // check for adjacent flooring.
        var tp = curPos;
        // L R U D
        if(ds_map_find_value(floorLocations, string(tp[0] - 32) + string(tp[1]))) then location += "l";
        if(ds_map_find_value(floorLocations, string(tp[0]) + string(tp[1] - 32))) then location += "u";
        if(ds_map_find_value(floorLocations, string(tp[0] + 32) + string(tp[1]))) then location += "r";
        if(ds_map_find_value(floorLocations, string(tp[0]) + string(tp[1]+32))) then  location += "d";
        
        // now choose the floor sprite based on where it needs to lead too.
        
        var imageIndex;
        if (location == "lurd"){
            imageIndex = 8;
        }
        else if(location =="lur"){
            imageIndex = 8;
        }
        else if(location =="lud"){
            imageIndex = 5;
        }
        else if(location =="lrd"){
            imageIndex = 7;
        }
        else if(location =="urd"){
            imageIndex = 6;
        }
        else if(location =="lu"){
            imageIndex = 5;
        }
        else if(location =="lr"){
            imageIndex = 9;
        }
        else if(location =="ld"){
            imageIndex = 2;
        }
        else if(location =="ur"){
            imageIndex = 6;
        }
        else if(location =="ud"){
            imageIndex = 4;
        }
        else if(location =="rd"){
            imageIndex = 3;
        }
        else if(location =="l"){
            imageIndex = 2;
        }
        else if(location =="u"){
            imageIndex = 4;
        }
        else if(location =="r"){
            imageIndex = 3;
        }
        else if(location =="d"){
            imageIndex = 1;
        }
        else{
            imageIndex = 1;
        }
        location = "";
        
        ds_map_add(floorLocations, string(curPos[0]) + string(curPos[1]), 1);
        var o = instance_create(curPos[0],curPos[1], obj_floor);
        o.image_index = imageIndex+2;
    }
    curPos[0] += 32;
    if(curPos[0] &gt; room_width+32){
        curPos[0] = 0;
        curPos[1] += 32;
    }
        
}
</string>
